<div class="tag-list">
  {%- for item in tags -%}
    <a href="{% block url %}#{% endblock %}">
     	{%- block tag -%}{{- item -}}{%- endblock -%}
    </a>{% if not loop.last %}, {% endif %}
  {%- endfor -%}
</div>
